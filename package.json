{
  "name": "@themoviedb/the-movie-db-store",
  "version": "1.0.0",
  "private": true,
  "description": "A package to initialise Redux store and reducers ü§π‚Äç‚ôÇ",
  "dependencies": {
    "axios": "^0.18.0",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "1.1.3",
    "dotenv": "^6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react-app": "^3.0.2",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "file-loader": "^2.0.0",
    "fs-extra": "3.0.1",
    "husky": "^1.1.1",
    "jest": "^23.6.0",
    "lodash-es": "^4.17.11",
    "object-assign": "^4.1.1",
    "promise": "^8.0.2",
    "raf": "3.4.0",
    "resolve": "^1.8.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "0.6.2",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9",
    "webpack-manifest-plugin": "^2.0.4",
    "whatwg-fetch": "2.0.3"
  },
  "peerDependencies": {
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "react": "^16.5.2",
    "react-redux": "^5.0.7"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run eslint:fix && npm run test:hook"
    }
  },
  "scripts": {
    "start": "node config/scripts/start.js",
    "build": "node config/scripts/build.js",
    "eslint": "eslint --max-warnings 0 ./src",
    "eslint:fix": "npm run eslint -- --fix",
    "test": "node config/scripts/test.js --env=jsdom",
    "test:hook": "npm test -- --forceExit",
    "prepush-hook": "npm run eslint:fix && npm run test:hook"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js}",
      "<rootDir>/src/**/?(*.)(spec|test).{js}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js)$"
    ],
    "moduleNameMapper": {
      "#(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "node",
      "mjs"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
